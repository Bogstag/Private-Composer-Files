{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock",
    "roots",
    "wordpress",
    "stack",
    "composer",
    "vagrant",
    "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://wp-languages.github.io"
    },
    {
      "type": "package",
      "package": {
        "name": "koodimonni-plugin-language/woocommerce-pdf-invoices-packing-slips-sv_SE",
        "type": "wordpress-language",
        "version": "1.5.39",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/translation/plugin/woocommerce-pdf-invoices-packing-slips/1.5.38/sv_SE.zip",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "koodimonni-theme-language/storefront-sv_SE",
        "type": "wordpress-language",
        "version": "2.1.6",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/translation/theme/storefront/2.1.6/sv_SE.zip",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "koodimonni-plugin-language/siteorigin-panels-sv_SE",
        "type": "wordpress-language",
        "version": "2.4.25",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/translation/plugin/siteorigin-panels/2.4.21/sv_SE.zip",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "koodimonni-plugin-language/akismet-sv_SE",
        "type": "wordpress-language",
        "version": "3.2",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/translation/plugin/akismet/3.2/sv_SE.zip",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "explodybits/hookr-plugin",
        "version": "master",
        "source": {
          "url": "https://github.com/explodybits/hookr-plugin.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "custom/pricing-deals-for-woocommerce",
        "type": "wordpress-plugin",
        "version": "1.1.6.7",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/pricing-deals-for-woocommerce.1.1.6.7.zip"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.7.*",
    "oscarotero/env": "^1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wpackagist-plugin/bugsnag": "1.3.*",
    "wpackagist-theme/storefront": "2.1.*",
    "koodimonni-theme-language/storefront-sv_SE": "2.1.*",
    "wpackagist-plugin/woocommerce": "2.6.*",
    "koodimonni-plugin-language/woocommerce-sv_se": "2.6.*",
    "wpackagist-plugin/autodescription": "2.8.*",
    "wpackagist-plugin/woocommerce-pdf-invoices-packing-slips": "1.5.39",
    "koodimonni-plugin-language/woocommerce-pdf-invoices-packing-slips-sv_SE": "1.5.38",
    "wpackagist-plugin/wp-force-ssl": "1.3.*",
    "koodimonni-language/sv_se": "*",
    "wpackagist-plugin/google-sitemap-generator": "4.0.*",
    "wpackagist-plugin/tiny-compress-images": "2.2.*",
    "wpackagist-plugin/woosidebars": "1.4.*",
    "wpackagist-plugin/wp-maintenance-mode": "2.0.9",
    "wpackagist-plugin/siteorigin-panels": "2.4.25",
    "koodimonni-plugin-language/siteorigin-panels-sv_SE": "2.4.23",
    "wpackagist-plugin/so-widgets-bundle": "1.8.*",
    "wpackagist-plugin/addon-so-widgets-bundle": "2.4.1",
    "wpackagist-plugin/ninja-forms": "3.0.*",
    "koodimonni-plugin-language/akismet-sv_SE": "3.2",
    "wpackagist-plugin/woocommerce-advanced-free-shipping": "1.0.11",
    "wpackagist-plugin/yith-woocommerce-custom-order-status": "1.0.9",
    "wpackagist-plugin/woocommerce-google-analytics-integration": "1.4.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5.1",
    "wpackagist-plugin/query-monitor": "2.13.*",
    "wpackagist-plugin/wp-maintenance-mode": "2.0.9",
    "explodybits/hookr-plugin": "dev-master",
    "wpackagist-plugin/debug-bar": "dev-trunk",
    "wpackagist-plugin/developer": "dev-trunk",
    "wpackagist-plugin/mailtrap-for-wp": "0.2.*",
    "custom/pricing-deals-for-woocommerce": "1.1.6.7"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp",
    "dropin-paths": {
      "web/app/languages/": [
        "vendor:koodimonni-language"
      ],
      "web/app/languages/plugins/": [
        "vendor:koodimonni-plugin-language"
      ],
      "web/app/languages/themes/": [
        "vendor:koodimonni-theme-language"
      ]
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs --ignore=web/wp/,vendor/ -n -s ."
    ]
  }
}
